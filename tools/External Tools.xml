      <option name="COMMAND" value="$ProjectFileDir$\node_modules\.bin\prettier.cmd" />
      <option name="PARAMETERS" value="--config prettier.config.js --write $FileDirRelativeToProjectRoot$/**/*.js" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>